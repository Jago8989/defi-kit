{
  "name": "defi-presets",
  "version": "0.1.1",
  "description": "Permission presets for Zodiac Roles covering interactions with DeFi protocols",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./eth": {
      "require": "./dist/eth.js",
      "import": "./dist/eth.mjs",
      "types": "./dist/eth.d.ts"
    },
    "./gor": {
      "require": "./dist/gor.js",
      "import": "./dist/gor.mjs",
      "types": "./dist/gor.d.ts"
    }
  },
  "typesVersions": {
    "*": {
      "index": [
        "./dist/eth.d.ts"
      ],
      "eth": [
        "./dist/eth.d.ts"
      ],
      "gor": [
        "./dist/gor.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "setup": "eth-sdk",
    "build": "rimraf dist && tsup --format esm,cjs",
    "dev": "tsup --format esm --watch"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^5.0.0",
    "@dethcrypto/eth-sdk": "^0.3.4",
    "@dethcrypto/eth-sdk-client": "^0.1.6",
    "ethers": "^5.7.2",
    "zod": "^3.21.4",
    "zodiac-roles-sdk": "^2.0.3"
  },
  "devDependencies": {
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "tsup": "^6.7.0",
    "typescript": "^5.0.4"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  }
}
